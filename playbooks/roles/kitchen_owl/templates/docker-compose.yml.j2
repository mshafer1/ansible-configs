services:
  front:
    image: tombursch/kitchenowl-web:latest
    restart: unless-stopped
    ports:
      - "127.0.0.1:{{ kitchen_owl_docker_port }}:80"
    depends_on:
      - back
  back:
    image: tombursch/kitchenowl-backend:latest
    restart: unless-stopped
    environment:
      - JWT_SECRET_KEY={{ kitchen_owl_javascript_web_token_secret_key }}
    volumes:
      - ./data:/data
